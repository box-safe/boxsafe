{
  "project": {
    "workspace": "./",
    "testDir": "./",
    "versionControl": {
      "before": true,
      "after": true,
      "generateNotes": true
    }
  },

  "model": {
    "primary": {
      "provider": "google",
      "name": "gemini-2.5-flash"
    },
    "fallback": [],
    "endpoint": null,
    "parameters": {}
  },

  "smartRotation": {
    "enabled": false,
    "simple": [],
    "complex": []
  },

  "limits": {
    "tokens": 100000,
    "loops": 2,
    "timeout": {
      "enabled": false,
      "duration": "1h",
      "notify": true
    }
  },

  "sandbox": {
    "enabled": true,
    "engine": "docker",
    "memory": "512m",
    "cpu": 0.5,
    "network": "none"
  },

  "commands": {
    "setup": "npm install",
    "run": "npm run dev",
    "test": null
  },

  "interface": {
    "channel": "terminal",
    "prompt": " crie um arquivo chamado love_test.js , nele adiciona o seguinte algoritimo, este algoritimo verifica se o usuario usa windows ou linux se for linux ele imprime no terminal 'I Love you' se for windows ele imprime 'I Hate you' commit o seguinte apos terminal 'second test with the agent done' .",
    "notifications": {
      "whatsapp": false,
      "telegram": false,
      "slack": false,
      "email": false
    }
  },

  "teach": {
    "urls": [],
    "files": []
  }
}