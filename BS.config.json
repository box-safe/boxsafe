{
  "project": {
    "workspace": "./src",
    "testDir": "./",
    "versionControl": {
      "before": true,
      "after": true,
      "generateNotes": true
    }
  },

  "model": {
    "primary": {
      "provider": "google",
      "name": "gemini-2.5-flash"
    },
    "fallback": [],
    "endpoint": null,
    "parameters": {}
  },

  "smartRotation": {
    "enabled": false,
    "simple": [],
    "complex": []
  },

  "limits": {
    "tokens": 100000,
    "loops": 2,
    "timeout": {
      "enabled": false,
      "duration": "1h",
      "notify": true
    }
  },

  "sandbox": {
    "enabled": true,
    "engine": "docker",
    "memory": "512m",
    "cpu": 0.5,
    "network": "none"
  },

  "commands": {
    "setup": "npm install",
    "run": "npm run dev",
    "test": null
  },

  "interface": {
    "channel": "terminal",
    "prompt": "Use Node.js fs/promises to create a folder named 'test' at the root of the workspace (./test). Inside it, create a file called 'test.md' and write '# hello world' as its content. Use mkdir and writeFile from fs/promises. After creating the files, read and print the content of test.md to verify it was created correctly.",
    "notifications": {
      "whatsapp": false,
      "telegram": false,
      "slack": false,
      "email": false
    }
  },

  "teach": {
    "urls": [],
    "files": []
  }
}